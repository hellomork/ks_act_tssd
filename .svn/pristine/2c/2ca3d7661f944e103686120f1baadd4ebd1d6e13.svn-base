          transform: translateX(-50%);
          background: url(../assets/img/hzact/img_title.png);
          background-position: center;
          background-size: 100%;
          background-repeat: no-repeat;
          
          h3{
            padding-top: 0.4rem;
            text-align: center;
            font-weight: bold;
            font-size: 0.32rem;
            color: #CF3F04;

          }
          p{
            margin-top: 0.3rem;
            text-align: center;
            font-size: 0.24rem;
            color: #CF3F04;
          }
        }
        .getprice{
          width: 100%;
          // height: 2rem;
          ul{
            display: table;
            width: 100%;
            height: 1rem;
            li{
              display:table-cell;
              height: 1rem;
              color: #CF3F04;
              font-size: 0.22rem;
              line-height: 1rem;
              &:nth-child(2){
                text-align: center;
                width: 1.26rem;
              }
              &:nth-child(3){
                text-align: right;
                width: 1.26rem;
              }
  
              
            }
            &:first-child{
              // margin-top: 17px;
            }
          }
  
        }

      }
    }
  }
