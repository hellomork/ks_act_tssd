@import "./variables.scss";
@media screen and (max-width: 750px) {
  .clearfix {
    &:after {
      visibility: hidden;
      display: block;
      font-size: 0;
      content: " ";
      clear: both;
      height: 0;
    }
  }
  .van-field__control{
    font-size: 0.28rem;
  }
  .van-uploader{
    width: 100%;
    margin-top: 0.29rem;
  }
  .van-uploader__input-wrapper{
    width: 100%;
  }
  .van-uploader__preview-image{
    width: 100%;
    height: 3.8rem;
  }
  .van-image__img{
    width: 100%;
    height: 3.8rem;
  }
  .van-uploader__preview{
    margin: 0!important
  }
  .van-uploader__preview-delete{
    width: .75rem;
    height: .75rem;
    background-image: url('../assets/imgwx/ic_off.png');
    background-repeat: no-repeat;
    background-size: 100%;
    border-radius:0;
    background-color: transparent;
    top: 0;
    right:0;
    &:before{
      content: none;
    }
  }
  html {
    font-size: 13.3vw;
  }

  .sortactive {
    background-color: $theme;
    color: $fontColor !important
  }

  .wxheadbg {
    .wx_header {
      overflow: hidden;
      width: 100%;
      height: 7rem;
      background: url(../assets/imgwx/hzact/banner.jpg);
      background-size: cover;
      // background-position: center;
      background-repeat: no-repeat;
      .readCount{
        margin-top: 4.75rem;
        text-align: center;
        span{
          font-size: 0.34rem;
          font-family: Microsoft YaHei;
          font-weight: bold;
          color: #B92617;
        }
      }
      .submit {
        // margin-top: 3.16rem;
        // width: 2.98rem;
        // height: 0.98rem;
        margin-top: 5.44rem;
        position: relative;
        // left: 0.34rem;
        // left: 50%;
        // transform: translateX(-50%);
        // background-color: #e62a36;

        .submit_i {
          width: 3.2rem;
          height: 0.96rem;
          // border: 1px solid white;
          background: #E6352B;
          box-shadow: 0px 0.08rem 0.26rem 0px rgba(230, 53, 43, 0.5);
          border-radius: 0.48rem;
          position: absolute;
          left: 50%;
          // top: 4.74rem;
          transform: translateX(-50%);
          text-align: center;

          span {
            display: inline-block;
            font-weight: 500;
            line-height: 0.96rem;
            font-size: 0.36rem;
            font-family: PingFang SC;
            font-weight: bold;
            color: #FFFFFF
          }

          img {
            width: 0.38rem;
            height: 0.38rem;
            position: relative;
            top: 0.08rem;
            left: 0.2rem;
          }
        }
      }

      .data {
        margin-top: 0.36rem;
        position: relative;
        left: 50%;
        transform: translateX(-50%);
        width: 5.25rem;
        height: 1rem;
        background-color: rgba($color: #ffffff, $alpha: 0.5);

        ul {
          display: table;
          width: 100%;

          li {
            position: relative;
            display: table-cell;
            // float: left;
            text-align: center;

            .line {
              h3 {
                font-weight: normal;
                font-family: Oswald-Light;
                font-size: 0.32rem;
                margin-top: 0.2rem;
              }

              p {
                margin-top: 0.13rem;
                font-size: 0.22rem;
              }

              ::after {
                content: '';
                width: 0.03rem;
                height: 0.47rem;
                position: absolute;
                left: 2.6rem;
                bottom: 0.1rem;
                background-color: rgba($color: #2f2119, $alpha: 0.2);
                opacity: 0.2;
              }
            }

            h3 {
              font-weight: normal;
              font-size: 0.32rem;
              margin-top: 0.2rem;
            }

            p {
              margin-top: 0.13rem;
              font-size: 0.22rem;
            }


          }
        }
      }
    }
  }

 

  // 通用样式
  .article {
    // display: inline-block;
    background-color: #F9F1EA;
    padding: 0.93rem 0.26rem 0.99rem 0.26rem;

    .intro {
      position: relative;
      width: 100%;
      // height: 14.38rem;
      border: 0.04rem solid $themeBg;
      background-color: $fontColor;
      border-radius: 0.3rem;
      padding: 0.86rem 0.3rem 0.44rem 0.3rem;

      .intro_title {
        width: 4.4rem;
        height: 0.9rem;
        position: absolute;
        left: 50%;
        transform: translateX(-50%);
        top: -0.43rem;

        img {
          width: 100%;
          height: 100%;
        }
      }
    }
  }
  .wx_award{
    .special{
      width: 100%;
    }
    .award{
      .awardlist{
        .cover{
          width: 2.46rem;
          height: 1.66rem;
          position: relative;
